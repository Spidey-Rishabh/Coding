//Link of the question-https://practice.geeksforgeeks.org/problems/row-with-max-1s0023/1
#include <bits/stdc++.h>
using namespace std;
class Solution{
public:
	int rowWithMax1s(vector<vector<int> > arr, int n, int m) {
	    // code here
	    int count=0,k;
	    for(int i=n-1;i>=0;i--){
	        int cnt=0;
            for(int j=0;j<m;j++){
                if(arr[i][j]==1) cnt++;
            }
            count=max(count,cnt);
            if(count==cnt) k=i;
	    }
	    if(count==0) return -1;
	    return k;
	}
};
int main() {
    int t;
    cin >> t;
    while (t--) {
        int n, m;
        cin >> n >> m;
        vector< vector<int> > arr(n,vector<int>(m));
        for (int i = 0; i < n; i++) {
            for (int j = 0; j < m; j++) {
                cin>>arr[i][j];
            }
        }
        Solution ob;
        auto ans=ob.rowWithMax1s(arr,n,m);
        cout<<ans<<"\n";
    }
    return 0;
}
