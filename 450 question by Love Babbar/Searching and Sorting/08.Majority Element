//Link of the question-https://practice.geeksforgeeks.org/problems/majority-element-1587115620/1
#include<bits/stdc++.h>
using namespace std;
class Solution{
  public:
    int majorityElement(int a[], int size){
        int maj_index = 0, count = 1; 
        for(int i=1;i<size;i++){ 
            if(a[maj_index]==a[i]) 
                count++; 
            else
                count--; 
            if(count==0){ 
                maj_index=i; 
                count=1; 
            } 
        } 
        if(count==1){
            if(size==1) return a[0];
            else return -1;
        }
        return a[maj_index]; 
    }
};
int main(){
    int t;
    cin >> t;
    while(t--){
        int n;
        cin >> n;
        int arr[n];
        for(int i = 0;i<n;i++){
            cin >> arr[i];
        }
        Solution obj;
        cout << obj.majorityElement(arr, n) << endl;
    }
    return 0;
}
